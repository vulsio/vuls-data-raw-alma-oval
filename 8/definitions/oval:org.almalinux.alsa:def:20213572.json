	"metadata": {
		"Title": "ALSA-2021:3572: nss and nspr security, bug fix, and enhancement update (Moderate)",
		"reference": [
			{
				"ref_id": "ALSA-2021:3572",
				"source": "ALSA",
				"ref_url": "https://errata.almalinux.org/8/ALSA-2021-3572.html"
