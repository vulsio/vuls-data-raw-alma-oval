	"metadata": {
		"Title": "ALSA-2021:4590: rust-toolset:rhel8 security update (Moderate)",
		"reference": [
			{
				"ref_id": "ALSA-2021:4590",
				"source": "ALSA",
				"ref_url": "https://errata.almalinux.org/8/ALSA-2021-4590.html"
			},
